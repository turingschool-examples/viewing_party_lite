<h1>Welcome to Viewing Party!</h1>

<% if current_user %>
  <h3>Current Users:</h3>
  <hr>
  <% @users.each do |user| %>
    <h3><%= "#{user.name}" %></h3>
    <p><%= user.email %></p>
    <hr>
  <% end %>
  <%= link_to "Log Out", "/logout", method: :delete %>
<% else %>
  <%= button_to "Log In", "/login", method: :get %><br>
  <%= button_to "Register", "/register", method: :get %><br>
<% end %>
