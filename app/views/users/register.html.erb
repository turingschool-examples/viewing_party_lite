<h1> New User Registration </h1>

<section id= "registration_form">

<%= form_with url: "/users", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :email%>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.submit "Register" %>
<% end %>

</section>