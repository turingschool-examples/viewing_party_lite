<%= form_with url: "/users", method: :post, local: true do |form| %>
  <%= form.label "Name" %>
  <%= form.text_field :name %>
  <%= form.label "Email" %>
  <%= form.text_field :email %>
  <%= form.label "Password" %>
  <%= form.text_field :password %>
  <%= form.label "Password Confirmation" %>
  <%= form.text_field :password_conf %>
  <%= form.submit "Register" %>
<% end %>
</div> </h2>