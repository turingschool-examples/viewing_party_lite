<%= form_with url: '/users', local: true do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <p><%= f.label :password %>
    <%= f.text_field :password %></p>

    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>

    <%= f.submit "Register"%>
<% end %>
