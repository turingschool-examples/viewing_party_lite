<section id = "header">
    <h1> <%= @user.name %> Dashboard</h1>
</section>

<section id = "discover_movies_button">
   <p> <%= button_to 'Discover Movies', "/users/#{@user.id}/discover", method: :get %></p>
</section>

<h3> Viewing Parties </h3> 
    <section id = "hosted-parties"> 
    <% @hosts_parties.each do |party| %>
        <p>When: <%= party.when.strftime('%Y-%m-%d') %> Time: <%= party.start_time.strftime('%H:%M') %>
     <% end %>
     </section>