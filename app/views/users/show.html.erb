<h1> Welcome, <%= @user.email %>! </h1>

<h2> <%= button_to "Discover Movies", "/users/#{@user.id}/discover", method: :get %> </h2>


<h2> Viewing Parties: </h2>
<% @user.viewing_parties.each do |party| %>
<div id="parties-<%=party.id%>">
   <p> <h4>Party ID: <%=link_to party.id, "/users/#{@user.id}/movies/#{@movie_id}/viewing_party/#{party.id}" %></h4>
   
    </p>
    <% end %>
    <br>
</div>