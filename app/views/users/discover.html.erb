
<%= button_to "Find Top Rated Movies", "/dashboard/movies", method: :get, params:{q: "top_rated"}%><br><br>

<%= form_with url: "/dashboard/movies", method: :get, local: true do |form| %>
  <%= form.label :keyword, "Search by Movie Title" %><br>
  <%= form.text_field :q %><br>
  <%= form.submit "Submit" %>
<% end %>