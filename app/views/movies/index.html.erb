<%=content_for :subtitle do%>
  <h3><%= @subtitle %></h3>
<%end%>

<%= render 'partials/discover_button'%>

<table id='movie-table' class='center'>
  <tr>
    <th>Title</th>
    <th>Rating</th>
  </tr>
  <%@movies.each do |movie|%>
  <tr class='movie' id='mov-<%=movie.id%>'>
    <td class='title'>
      <%= link_to movie.title, "/users/#{@user.id}/movies/#{movie.id}"%>
    </td>
    <td class='rating'>
      <%= movie.vote_avg %>
    </td>
  </tr>
  <%end%>

    
</table>