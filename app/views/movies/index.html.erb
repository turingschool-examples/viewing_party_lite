<h1>Movie Results </h1>
<%= link_to 'Back to Discover Page', discover_index_path %>
<% @movies.each do |movie| %>
  <ul class="movies">
    <li class="Title">Title: <%= link_to "#{movie.title}", movie_show_path(@user.id, movie.id), method: :get%></li>
    <li class="Vote Average">Vote Average: <%= movie.vote_average%></li>
</ul>
<% end %>
